CREATE TABLE Products (ID int NOT NULL AUTO_INCREMENT, ItemID varchar(30)NOT NULL, ProductName varchar(30) NOT NULL, DepartmentName varchar(30) NOT NULL,Price varchar(30) NOT NULL, StockQuantity varchar(30) NOT NULL, PRIMARY KEY (ID));
